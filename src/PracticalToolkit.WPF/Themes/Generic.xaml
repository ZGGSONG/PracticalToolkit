    <Color x:Key="WD.BlackColor" po:Freeze="True">#000000</Color>
    <SolidColorBrush x:Key="WD.BlackSolidColorBrush" po:Freeze="True" Color="{StaticResource WD.BlackColor}" />
    <Color x:Key="WD.BackgroundColor" po:Freeze="True">#FFFFFF</Color>
    <SolidColorBrush x:Key="WD.BackgroundSolidColorBrush" po:Freeze="True" Color="{StaticResource WD.BackgroundColor}" />
    <Color x:Key="WD.PrimaryNormalColor" po:Freeze="True">#409EFF</Color>
    <SolidColorBrush x:Key="WD.PrimaryNormalSolidColorBrush" po:Freeze="True" Color="{StaticResource WD.PrimaryNormalColor}" />
    <Color x:Key="WD.RegularTextColor" po:Freeze="True">#606266</Color>
    <SolidColorBrush x:Key="WD.RegularTextSolidColorBrush" po:Freeze="True" Color="{StaticResource WD.RegularTextColor}" />
    <DropShadowEffect x:Key="WD.PopupShadowDepth" BlurRadius="12" Opacity="0.1" ShadowDepth="2" Color="{StaticResource WD.BlackColor}" />
    <Color x:Key="WD.PrimaryTextColor" po:Freeze="True">#909399</Color>
    <SolidColorBrush x:Key="WD.SecondaryTextSolidColorBrush" po:Freeze="True" Color="{StaticResource WD.PrimaryTextColor}" />
    <Color x:Key="WD.DangerColor" po:Freeze="True">#F56C6C</Color>
    <SolidColorBrush x:Key="WD.DangerSolidColorBrush" po:Freeze="True" Color="{StaticResource WD.DangerColor}" />
    <Color x:Key="WD.SuccessColor" po:Freeze="True">#67C23A</Color>
    <SolidColorBrush x:Key="WD.SuccessSolidColorBrush" po:Freeze="True" Color="{StaticResource WD.SuccessColor}" />
    <Style x:Key="WD.PathButton" TargetType="{x:Type Button}">
        <Setter Property="FrameworkElement.FocusVisualStyle" Value="{x:Null}" />
        <Setter Property="FrameworkElement.OverridesDefaultStyle" Value="True" />
        <Setter Property="HorizontalContentAlignment" Value="Center" />
        <Setter Property="VerticalContentAlignment" Value="Center" />
        <Setter Property="BorderThickness" Value="1" />
        <Setter Property="Padding" Value="6" />
        <Setter Property="Padding" Value="8" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type Button}">
                    <Border Background="#00FFFFFF">
                        <ContentPresenter Name="PART_ContentPresenter" Margin="{TemplateBinding Control.Padding}" HorizontalAlignment="{TemplateBinding Control.HorizontalContentAlignment}" VerticalAlignment="{TemplateBinding Control.VerticalContentAlignment}" Opacity=".8" />
                    </Border>
                    <ControlTemplate.Triggers>
                        <Trigger Property="UIElement.IsMouseOver" Value="True">
                            <Setter TargetName="PART_ContentPresenter" Property="UIElement.Opacity" Value="1" />
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="WD.PathRadioButton" TargetType="{x:Type RadioButton}">
        <Setter Property="FrameworkElement.FocusVisualStyle" Value="{x:Null}" />
        <Setter Property="FrameworkElement.OverridesDefaultStyle" Value="True" />
        <Setter Property="HorizontalContentAlignment" Value="Center" />
        <Setter Property="VerticalContentAlignment" Value="Center" />
        <Setter Property="BorderThickness" Value="1" />
        <Setter Property="Padding" Value="8" />
        <Setter Property="UIElement.SnapsToDevicePixels" Value="True" />
        <Setter Property="FrameworkElement.UseLayoutRounding" Value="True" />
        <Setter Property="FrameworkElement.OverridesDefaultStyle" Value="True" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type RadioButton}">
                    <Border Background="#00FFFFFF">
                        <ContentPresenter Name="PART_ContentPresenter" Margin="{TemplateBinding Control.Padding}" HorizontalAlignment="{TemplateBinding Control.HorizontalContentAlignment}" VerticalAlignment="{TemplateBinding Control.VerticalContentAlignment}" Opacity=".8" />
                    </Border>
                    <ControlTemplate.Triggers>
                        <Trigger Property="ToggleButton.IsChecked" Value="True">
                            <Setter TargetName="PART_ContentPresenter" Property="UIElement.Opacity" Value="1" />
                        </Trigger>
                        <Trigger Property="UIElement.IsMouseOver" Value="True">
                            <Setter TargetName="PART_ContentPresenter" Property="UIElement.Opacity" Value="1" />
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <ControlTemplate x:Key="WD.PART_DrawArrow" TargetType="{x:Type Control}">
        <Grid>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="*" />
                <ColumnDefinition Width="15" />
            </Grid.ColumnDefinitions>
            <Path Height="8" Margin="0,0,-7,0" HorizontalAlignment="Left" VerticalAlignment="Center" Fill="{TemplateBinding Control.Background}" Stretch="Fill" Data="{StaticResource WD.ArrowBranchesGeometry}" />
            <Path Grid.Column="1" Width="15" Height="15" Fill="{TemplateBinding Control.Background}" Stretch="Fill" Data="{StaticResource WD.ArrowGeometry}" />
        </Grid>
    </ControlTemplate>
    <Style x:Key="WD.ScreenCutRectangleStyle" TargetType="{x:Type Rectangle}">
        <Setter Property="Fill" Value="{DynamicResource WD.BlackSolidColorBrush}" />
        <Setter Property="UIElement.Opacity" Value=".3" />
    </Style>
    <Style x:Key="WD.ScreenCutEllipseStyle" TargetType="{x:Type Ellipse}">
        <Setter Property="Width" Value="15" />
        <Setter Property="Height" Value="15" />
        <Setter Property="Fill" Value="{DynamicResource WD.BackgroundSolidColorBrush}" />
        <Setter Property="Stroke" Value="{DynamicResource WD.PrimaryNormalSolidColorBrush}" />
        <Setter Property="StrokeThickness" Value="2" />
    </Style>
    <Style x:Key="WD.ScreenShot" TargetType="{x:Type controls:ScreenShot}">
        <Setter Property="Window.WindowState" Value="Maximized" />
        <Setter Property="Window.WindowStyle" Value="None" />
        <Setter Property="Window.Title" Value="WPFDevelopers.ScreenShot" />
        <Setter Property="Window.Topmost" Value="True" />
        <Setter Property="Window.ResizeMode" Value="NoResize" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type controls:ScreenShot}">
                    <AdornerDecorator>
                        <Canvas Name="PART_Canvas" Width="{Binding Source={x:Static SystemParameters.PrimaryScreenWidth}}" Height="{Binding Source={x:Static SystemParameters.PrimaryScreenHeight}}">
                            <Rectangle Name="PART_RectangleLeft" Style="{StaticResource WD.ScreenCutRectangleStyle}" />
                            <Rectangle Name="PART_RectangleTop" Style="{StaticResource WD.ScreenCutRectangleStyle}" />
                            <Rectangle Name="PART_RectangleRight" Style="{StaticResource WD.ScreenCutRectangleStyle}" />
                            <Rectangle Name="PART_RectangleBottom" Style="{StaticResource WD.ScreenCutRectangleStyle}" />
                            <Border Name="PART_Border" Background="#00FFFFFF" BorderThickness="2" Cursor="SizeAll" BorderBrush="{StaticResource WD.PrimaryNormalSolidColorBrush}" />
                            <Border Name="PART_EditBar" Panel.ZIndex="99" Visibility="Hidden" Background="{StaticResource WD.BackgroundSolidColorBrush}" Effect="{StaticResource WD.PopupShadowDepth}">
                                <WrapPanel Margin="10,5" VerticalAlignment="Center">
                                    <RadioButton Name="PART_RadioButtonRectangle" Margin="4,0" Style="{DynamicResource WD.PathRadioButton}" ToolTip="方框">
                                        <Path Width="18" Height="18" Stretch="Fill" Data="{StaticResource WD.RectangleGeometry}" Fill="{StaticResource WD.RegularTextSolidColorBrush}" />
                                    </RadioButton>
                                    <RadioButton Name="PART_RadioButtonEllipse" Margin="4,0" ToolTip="椭圆" Style="{StaticResource WD.PathRadioButton}">
                                        <Ellipse Width="19" Height="19" SnapsToDevicePixels="True" StrokeThickness="1.5" UseLayoutRounding="True" Stroke="{StaticResource WD.RegularTextSolidColorBrush}" />
                                    </RadioButton>
                                    <RadioButton Name="PART_RadioButtonArrow" Margin="4,0" Style="{DynamicResource WD.PathRadioButton}" ToolTip="箭头">
                                        <Path Width="18" Height="18" Stretch="Fill" Stroke="#00FFFFFF" StrokeThickness="3" Data="{StaticResource WD.ArrowRightTopGeometry}" Fill="{StaticResource WD.RegularTextSolidColorBrush}" />
                                    </RadioButton>
                                    <RadioButton Name="PART_RadioButtonInk" Margin="4,0" ToolTip="画笔" Style="{StaticResource WD.PathRadioButton}">
                                        <Path Width="18" Height="18" Stretch="Fill" Data="{StaticResource WD.InkGeometry}" Fill="{StaticResource WD.RegularTextSolidColorBrush}" />
                                    </RadioButton>
                                    <RadioButton Name="PART_RadioButtonText" Margin="4,0" ToolTip="文字" Style="{StaticResource WD.PathRadioButton}">
                                        <Path Width="18" Height="18" Stretch="Fill" Data="{StaticResource WD.TextGeometry}" Fill="{StaticResource WD.RegularTextSolidColorBrush}" />
                                    </RadioButton>
                                    <Rectangle Width="1" Height="18" Margin="4,0" Stroke="{StaticResource WD.SecondaryTextSolidColorBrush}" />
                                    <Button Name="PART_ButtonSave" Margin="4,0" ToolTip="保存" Style="{StaticResource WD.PathButton}">
                                        <Path Width="18" Height="18" Stretch="Fill" Data="{StaticResource WD.SaveGeometry}" Fill="{StaticResource WD.RegularTextSolidColorBrush}" />
                                    </Button>
                                    <Button Name="PART_ButtonCancel" Margin="4,0" ToolTip="取消" Style="{StaticResource WD.PathButton}">
                                        <Path Width="14" Height="14" Stretch="Fill" Data="{StaticResource WD.CancelGeometry}" Fill="{StaticResource WD.DangerSolidColorBrush}" />
                                    </Button>
                                    <Button Name="PART_ButtonComplete" Margin="4,0" ToolTip="完成" Style="{StaticResource WD.PathButton}">
                                        <Path Width="20" Height="15" Stretch="Fill" Data="{StaticResource WD.CompleteGeometry}" Fill="{StaticResource WD.SuccessSolidColorBrush}" />
                                    </Button>
                                </WrapPanel>
                            </Border>
                            <Popup Name="PART_Popup" AllowsTransparency="True" Placement="Bottom" VerticalOffset="13">
                                <Border Name="PART_PopupBorder" Margin="10,30,10,10" CornerRadius="3" Effect="{StaticResource WD.PopupShadowDepth}">
                                    <Grid>
                                        <Path Width="10" Margin="0,-8,0,0" VerticalAlignment="Top" SnapsToDevicePixels="True" Stretch="Uniform" UseLayoutRounding="True" Data="{StaticResource WD.UpperTriangleGeometry}" Fill="{StaticResource WD.BackgroundSolidColorBrush}" />
                                        <WrapPanel Name="PART_WrapPanelColor" Margin="10" VerticalAlignment="Center">
                                            <RadioButton Margin="4,0" Background="#FFFF0000" IsChecked="True" Style="{DynamicResource WD.ColorRadioButton}" />
                                            <RadioButton Margin="4,0" Background="#FF1E90FF" Style="{DynamicResource WD.ColorRadioButton}" />
                                            <RadioButton Margin="4,0" Background="#FF32CD32" Style="{DynamicResource WD.ColorRadioButton}" />
                                            <RadioButton Margin="4,0" Background="#FFFFFF00" Style="{DynamicResource WD.ColorRadioButton}" />
                                        </WrapPanel>
                                    </Grid>
                                </Border>
                            </Popup>
                        </Canvas>
                    </AdornerDecorator>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="{x:Type controls:ScreenShot}" BasedOn="{StaticResource WD.ScreenShot}" TargetType="{x:Type controls:ScreenShot}" />
    <!--#endregion-->
</ResourceDictionary>